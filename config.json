{
  "mandatory_directories": [
    "data/",
    "output/",
    "data/train/images",
    "data/train/masks",
    "data/test/images"
  ],
  "filepaths": {
    "train_image": "data/train/images/{}.png",
    "test_image": "data/test/images/{}.png",
    "train_mask": "data/train/masks/{}.png",
    "depths_df": "data/depths.csv",
    "train_df": "data/train.csv",
    "saved_train": "output/train.pk",
    "saved_test": "output/test.pk",
    "logfile": "output/log-{}",
    "submission": "output/submission.csv"
  },
  "models_to_include": [
    "unet_resnet"
  ],
  "model_parameters": {
    "unet_resnet": {
      "final": false,
      "use_saved_model": true,
      "save_path": "output/model1.model",
      "optimal_cutoff": 0.5,
      "k_folds": 5,
      "kernel_initializer": "glorot_normal",
      "neurons_initial": 16,
      "batchnorm_momentum": 0.99,
      "optimizer_stage1": "adam",
      "lr_stage1": 0.01,
      "optimizer_momentum_stage1": 0.0,
      "optimizer_stage2": "adam",
      "lr_stage2": 0.01,
      "optimizer_momentum_stage2": 0.0,
      "early_stopping": 20,
      "batch_size_stage1": 32,
      "batch_size_stage2": 32,
      "epochs_stage1": 50,
      "epochs_stage2": 50,
      "dropout_expansion": true,
      "dropout_ratios_expansion": [0.25, 0.50, 0.50, 0.50],
      "dropout_contraction": true,
      "dropout_ratios_contraction": [0.50, 0.50, 0.50, 0.50]
    },
    "resnet50": {
      "final": false,
      "use_saved_model": true,
      "save_path": "output/resnet50.model",
      "pretrained_weights_path": "data/resnet50baseweights.h5",
      "optimal_cutoff": 0.5,
      "k_folds": 5,
      "kernel_initializer": "he_normal",
      "neurons_initial": 16,
      "batchnorm_momentum": 0.99,
      "optimizer": "adam",
      "lr": 0.01,
      "optimizer_momentum": 0.0,
      "early_stopping": 20,
      "batch_size_stage": 32,
      "epochs": 50
    }
  }
}